  },
  "devDependencies": {
    "@bonniernews/eslint-config": "^1.2.0",
    "c8": "^8.0.1",
    "chai": "^4.3.7",
    "depcheck": "^1.4.3",
    "eslint": "^8.57.0",
    "eslint-plugin-json": "^3.1.0",
